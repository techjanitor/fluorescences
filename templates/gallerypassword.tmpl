{{ template "base" .}}
<div class="admin">
  <div class="admin_title">Please insert the gallery key</div>
    <form action="/comic/{{.ID}}/1" method="get" enctype="multipart/form-data">
        <div class="form-group">
            <input type="text" class="form-control gallery_key" name="key" placeholder="Key" />
        </div>
        <div class="form-group">
            <button class="button button-default form-control" type="submit" value="Submit">Confirm</button>
        </div>
    </form>
</div>
{{ template "footer" .}}
